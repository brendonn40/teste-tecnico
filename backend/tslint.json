{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "rules": {
      "variable-name": [true, "allow-leading-underscore"],
      "no-console": false,
      "no-submodule-imports": [
        true,
        "source-map-support",
        "module-alias",
        "@services",
        "@controllers",
        "@config",
        "@entities"
      ],
      "no-implicit-dependencies": [true, "dev", ["@services", "@controllers", "@config", "@entities"]],
      "no-boolean-literal-compare": false
    }
  }